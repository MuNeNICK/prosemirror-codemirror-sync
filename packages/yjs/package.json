  "keywords": ["prosemirror", "codemirror", "yjs", "sync", "bridge"],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "y-prosemirror": "^1.3.7",
    "y-protocols": "^1.0.7",
